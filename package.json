{
  "name": "fivem-react-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:browser": "vite --mode ui",
    "dev:game": "vite build --watch --mode ui",
    "build:resource": "vite build --mode resource",
    "build:ui": "vite build --mode ui",
    "build": "npm run build:resource && npm run build:ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mantine/core": "^7.2.1",
    "@mantine/dates": "^7.2.1",
    "@mantine/form": "^7.2.1",
    "@mantine/hooks": "^7.2.1",
    "@mantine/notifications": "^7.2.1",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-node": "^3.0.2"
  },
  "devDependencies": {
    "@citizenfx/client": "2.0.7080-1",
    "@citizenfx/server": "2.0.7080-1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "postcss-preset-mantine": "^1.9.1",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
